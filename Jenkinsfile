pipeline
{
    agent any
    stages
    {
        stage('Checkout')
        {
            steps
            {
                echo "Checking out files form Github"
            }
        }
        stage('Build')
        {
            steps
            {
                echo "Building the project"
            }
        }
        stage('Test')
        {
            steps
            {
                echo "Testing the project"
            }
        }
        stage('Deployment')
        {
            steps
            {
                echo "Deployment the project"
            }
        }
    }
}